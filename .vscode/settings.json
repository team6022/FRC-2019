{
  "java.configuration.updateBuildConfiguration": "automatic",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "bin/": true,
    ".classpath": true,
    ".project": true
  },
  "wpilib.online": true,
  "editor.renderWhitespace": "boundary",
  "files.trimTrailingWhitespace": true,
  "editor.insertSpaces": false,
  "editor.tabCompletion": "on"
}
